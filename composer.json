{
    "name": "yii-extension/tailwind",
    "type": "library",
    "description": "Yii Extension Tailwind",
    "keywords": [
        "yii",
        "extension",
        "tailwind"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "foxy/foxy": "^1.1@dev",
        "yii-extension/asset-tailwind": "^1.0",
        "yii-extension/simple-widget": "^1.0",
        "yiisoft/assets": "^1.0",
        "yiisoft/files": "^1.0",
        "yiisoft/html": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.8",
        "vimeo/psalm": "^4.8",
        "yiisoft/var-dumper": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Tailwind\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Tailwind\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
